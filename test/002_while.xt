//测试0.02_while：0.02_if基础上加上while语句，不支持 || && 等
//条件支持 > < == >= <=

//计算1加到100的和
let s=0;
let i=0
while(i<=100)
{
    s=s+i;
    i=i+1
}

print(s,"\n")
i=0;

//输出所有3的倍数
while(i<=100)
{
    # let j=i%3;
    # comment
    if(i % 3 == 0){
        print(i," ")
    }
    i=i+1
}
