//测试0.02_while：0.02_if基础上加上while语句，不支持 || && 等
//条件支持 > < == >= <=

//计算1加到100的和
let s=0;

let i=0
while(i<=100)
{
    s=s+i;
    i=i+1
}

print(s,"\n")
i=1;

//输出50内不是3的倍数,测试break和continue
while(i<=100)
{
    if(i>50){break}
    // print("#",i,"#\n")
    # let j=i%3;
    # comment
    if(i%3==0){i=i+1;continue;}
    {
        print(i,"\n")
    }
    i=i+1
}
